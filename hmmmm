    if BunnyHopActive and Humanoid.WalkSpeed ~= getgenv().WalkSpeedValue then
        Humanoid.WalkSpeed = getgenv().WalkSpeedValue
    end
end)

local FlyToggle = Tab:CreateToggle({
    Name = "Fly/Unfly",
    CurrentValue = false,
    Flag = "FlyToggle",
    Callback = function(Value)
        if Value then
            if FLYING == false then
                sFLY()
            end
        else
            if FLYING == true then
                NOFLY()
            end
        end
    end,
})


