local function chonk9677(inputText)
    local transformedWords = {}

    for word in inputText:gmatch("%S+") do
        -- Apply the transformation from transformText here:
        word = word:gsub("l", "I")
        
        if #word > 2 then
            word = word:sub(1, 2) .. "êœ—" .. word:sub(3)
        end

        table.insert(transformedWords, word)
    end

    return table.concat(transformedWords, "")
end


	
