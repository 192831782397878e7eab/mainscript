    Name = "Fly keybind",
    CurrentKeybind = "F", 
    HoldToInteract = false,
    Flag = "FlyKeybind", 
    Callback = function(Keybind)
        if FLYING then
            NOFLY()
        else
            sFLY()
        end
    end,
})

