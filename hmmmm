    character = newCharacter
    humanoid = character:WaitForChild("Humanoid")

    humanoid.Died:Connect(duh7810)

    anim.AnimationId = bleh2621(character) and "rbxassetid://698251653" or "rbxassetid://72042024"

    if jorkin then
        task.wait(1)
        task.spawn(skrunkle1059)
    end
end)

